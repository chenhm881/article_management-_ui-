{
  "name": "book-blog-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.6.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/marked": "^3.0.0",
    "@types/node": "^12.20.19",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.8",
    "antd": "^4.16.12",
    "axios": "^0.21.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "concurrently": "^6.3.0",
    "dotenv-cli": "^4.1.0",
    "highlight.js": "^11.2.0",
    "js-cookie": "^3.0.0",
    "marked": "^3.0.2",
    "nodemon": "^2.0.14",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-quill": "^2.0.0-beta.4",
    "react-redux": "^7.2.4",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.3.5",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start:local": "dotenv -e .env/.env.local concurrently \" nodemon --ignore 'public/*' server/server.js\" \"npm run start\"",
    "start:dev": "dotenv -e .env/.env.development react-scripts start",
    "start:prod": "dotenv -e .env/.env.production react-scripts start",
    "build": "dotenv -e .env/.env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ]
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/js-cookie": "^2.2.7",
    "@types/react-router-config": "^5.0.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "less": "^4.1.2",
    "less-loader": "^10.0.1",
    "node-sass": "^6.0.1"
  }
}
